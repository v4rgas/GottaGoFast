{
  "manifest_version": 2,
  "name": "GottaGoFast",
  "version": "1.0",
  "description": "Toma cursos a la velocidad del sonido",
  "content_scripts": [
    {
      "matches": [
        "*://ssb.uc.cl/ERPUC/*>"
      ],
      "js": [
        "ggf.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Go Fast",
    "default_popup": "popup.html"
  }
}
